{
  "name": "finance-tracker-v2.0",
  "version": "2.0.0",
  "description": "Personal finance management system via Telegram bot with AI-powered natural language processing",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "convex dev",
    "build": "tsc && convex deploy",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,json,md}\""
  },
  "keywords": [
    "finance",
    "telegram",
    "convex",
    "ai"
  ],
  "author": "Amr",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.20",
    "@ai-sdk/groq": "^2.0.24",
    "@sentry/node": "^8.33.1",
    "ai": "^5.0.68",
    "convex": "^1.16.5",
    "date-fns": "^4.1.0",
    "pino": "^9.4.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/node": "^20.16.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.12.0",
    "playwright": "^1.56.0",
    "prettier": "^3.3.3",
    "typescript": "^5.9.3",
    "vitest": "^2.1.2"
  }
}
